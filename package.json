    "@types/mongoose": "^5.2.7",
    "@types/node": "^10.9.2",
    "@types/uuid": "^3.4.3",
    "cors": "^2.8.4",
    "cypress": "^3.0.3",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/jquery": "^3.3.6",
