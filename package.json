{
  "name": "optiplan",
  "version": "1.0.0",
  "description": "UofT Timetable Generator",
  "main": "api/index.js",
  "scripts": {
    "test": "chmod +x ./test/test.sh && sh ./test/test.sh",
    "start:ui": "nodemon --ext html,css,js,ts --config ui/nodeman-ui-config.json --ignore ui-build/ ui/index.js",
